{
  "name": "pstype",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "clean": "rimraf dist",
    "start": "nodemon \"./dist/www.js\"",
    "start2": "npm-run-all --parallel watch:build watch:server",
    "watch:build": "webpack --watch",
    "watch:server": "nodemon \"./dist/www.js\" --watch \"./dist\"",
    "webpack": "npm run clean && webpack --config webpack.config.js",
    "install": "npm i -g nodemon"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "body-parser": "~1.17.1",
    "debug": "~2.6.3",
    "express": "~4.15.2",
    "express-jwt": "^5.3.0",
    "jsonwebtoken": "^7.4.3",
    "jwt-decode": "^2.2.0",
    "mongoose": "^4.10.4",
    "mongoose-bcrypt": "^1.4.2",
    "mongoose-unique-validator": "^1.0.5",
    "morgan": "~1.8.1",
    "nodemon": "^1.10.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.11.0",
    "mocha": "^4.0.1",
    "npm-run-all": "^4.1.2",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4",
    "webpack-node-externals": "^1.6.0"
  }
}
